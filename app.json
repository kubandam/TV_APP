{
  "expo": {
    "name": "TVApp",
    "slug": "tvapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.tvapp.app",
      "appleTeamId": "5FXWZAASLW"
    },
    "android": {
      "package": "com.tvapp.app"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "tvapp",
    "fonts": [
      {
        "asset": "assets/fonts/AVALONT.ttf",
        "family": "Avalon-Thin"
      },
      {
        "asset": "assets/fonts/AVALONN.ttf",
        "family": "Avalon-Normal"
      },
      {
        "asset": "assets/fonts/AVALONI.ttf",
        "family": "Avalon-Italic"
      },
      {
        "asset": "assets/fonts/AVALONB.ttf",
        "family": "Avalon-Bold"
      }
    ]
  }
}
